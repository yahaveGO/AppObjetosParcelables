<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity" >

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_start"
        android:layout_marginTop="@dimen/size_top"
        android:layout_marginEnd="@dimen/size_end"
        android:gravity="center"
        android:text="@string/titulo_parcelable"
        android:textColor="#FF1A237E"
        android:textSize="@dimen/titulo_size" />

    <Button
        android:id="@+id/button5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_start"
        android:layout_marginTop="@dimen/size_top"
        android:layout_marginEnd="@dimen/size_start"
        android:background="#FF512DA8"
        android:onClick="pasarObjParcelable"
        android:text="@string/paso_obj"
        android:textAllCaps="false"
        android:textColor="#FFFFFF" />

    <Button
        android:id="@+id/button6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_start"
        android:layout_marginTop="@dimen/size_top"
        android:layout_marginEnd="@dimen/size_start"
        android:background="#FF512DA8"
        android:onClick="pasarListaParcelable"
        android:text="@string/paso_obj_lista"
        android:textAllCaps="false"
        android:textColor="#FFFFFF" />

    <Button
        android:id="@+id/button7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_start"
        android:layout_marginTop="@dimen/size_top"
        android:layout_marginEnd="@dimen/size_start"
        android:background="#FF512DA8"
        android:onClick="pasarEstante"
        android:text="@string/paso_obj_obj"
        android:textAllCaps="false"
        android:textColor="#FFFFFF" />

    <Button
        android:id="@+id/button8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/size_start"
        android:layout_marginTop="@dimen/size_top"
        android:layout_marginEnd="@dimen/size_start"
        android:background="#FF512DA8"
        android:onClick="retornarObj"
        android:text="@string/retorno_obj"
        android:textAllCaps="false"
        android:textColor="#FFFFFF" />
</LinearLayout>